# Browser
HEADLESS=true
BROWSER_TYPE=chromium
SLOW_MO=0

# Proxy (optional)
# PROXY_LIST=socks5://user:pass@host:port,http://user:pass@host:port
# PROXY_STRATEGY=round_robin

# API export (optional)
# API_ENDPOINT=https://your-api.example.com/ingest
# API_TOKEN=your-bearer-token

# Rate limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_PERIOD=60

# Output
OUTPUT_DIR=output

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=console
